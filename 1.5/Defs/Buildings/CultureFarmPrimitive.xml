<Defs>
	<ThingDef ParentName="BuildingBase">
    <defName>SC_CultureFarmPrimitive</defName>
    <label>Primitive culture farm</label>
    <description>A primitive building to farm strain culture. Requires nutritions, reasonable temperature and hard work to function.</description>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.45</fillPercent>
    <pathCost>42</pathCost>
    <castEdgeShadows>true</castEdgeShadows>
    <minifiedDef>MinifiedThing</minifiedDef>
    <drawerType>MapMeshAndRealTime</drawerType>
    <tickerType>Normal</tickerType>
    <altitudeLayer>Building</altitudeLayer>
    <thingCategories>
      <li>BuildingsProduction</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Building/Production/FermentingBarrel</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
      </damageData>
      <shadowData>
        <volume>(0.75, 0.35, 0.33)</volume>
      </shadowData>
    </graphicData>
    <size>(1, 1)</size>
    <rotatable>true</rotatable>
    <canOverlapZones>false</canOverlapZones>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0, 0, -1)</interactionCellOffset>
    <defaultPlacingRot>North</defaultPlacingRot>
    <uiOrder>2720</uiOrder>
    <designationCategory>Production</designationCategory>
    <constructEffect>ConstructWood</constructEffect>
    <statBases>
      <WorkToBuild>600</WorkToBuild>
      <Mass>10</Mass>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <costList>
      <Steel>10</Steel>
      <WoodLog>30</WoodLog>
    </costList>
    <building>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
      <ai_chillDestination>false</ai_chillDestination>
      <destroySound>BuildingDestroyed_Wood_Small</destroySound>
      <haulToContainerDuration>120</haulToContainerDuration>
      <!-- <fixedStorageSettings>
        <filter>
          <categories>
            <li>Foods</li>
          </categories>
          <specialFiltersToDisallow>
            <li>AllowPlantFood</li>
          </specialFiltersToDisallow>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <filter>
          <categories>
            <li>Foods</li>
          </categories>
          <disallowedCategories>
            <li>EggsFertilized</li>
          </disallowedCategories>
          <disallowedThingDefs>
            <li>InsectJelly</li>
            <li>MealLavish</li>
            <li>MealLavish_Veg</li>
            <li>MealLavish_Meat</li>
            <li>HemogenPack</li>
            <li>Chocolate</li>
          </disallowedThingDefs>
        </filter>
      </defaultStorageSettings> -->
    </building>
    <constructionSkillPrerequisite>4</constructionSkillPrerequisite>
    <comps>
      <li Class="StrainCultures.Comps.CompProperties_PrimitiveCultureFarm">
        <ticksPerCulture>50</ticksPerCulture>
        <fuelPerCulture>0.1</fuelPerCulture>
      </li>
      <li Class="CompProperties_TemperatureRuinable">
        <minSafeTemperature>-1</minSafeTemperature>
        <maxSafeTemperature>32</maxSafeTemperature>
        <progressPerDegreePerTick>0.00001</progressPerDegreePerTick>
      </li>
      <li Class="CompProperties_Refuelable">
        <fuelCapacity>150.0</fuelCapacity>
        <targetFuelLevelConfigurable>true</targetFuelLevelConfigurable>
        <initialConfigurableTargetFuelLevel>75</initialConfigurableTargetFuelLevel>
        <externalTicking>true</externalTicking>
        <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed>
        <fuelFilter>
          <thingDefs>
            <li>Kibble</li>
          </thingDefs>
        </fuelFilter>
        <!-- <consumeFuelOnlyWhenUsed>true</consumeFuelOnlyWhenUsed> -->
        <autoRefuelPercent>30</autoRefuelPercent>
        <showFuelGizmo>true</showFuelGizmo>
        <drawOutOfFuelOverlay>true</drawOutOfFuelOverlay>
        <drawFuelGaugeInMap>false</drawFuelGaugeInMap>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
    </li>
    </comps>
  </ThingDef>
</Defs>